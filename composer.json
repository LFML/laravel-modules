{
  "name": "lfml/laravel-modules",
  "description": "Laravel Module management",
  "keywords": [
    "modules",
    "laravel",
    "Laravel",
    "module",
    "rad"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Anonimo",
      "email": "anonimo@gmail.com",
      "homepage": "https://examples.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^3.8",
    "friendsofphp/php-cs-fixer": "^2.14",
    "laravel/framework": "5.8.*",
    "phpstan/phpstan": "^0.9.2",
    "spatie/phpunit-snapshot-assertions": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Modules\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Laravel\\Modules\\Tests\\": "tests",
      "Modules\\Recipe\\": "tests/stubs/valid/Recipe"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\Modules\\LaravelModulesServiceProvider",
        "Laravel\\Modules\\Providers\\BootstrapServiceProvider"
      ],
      "aliases": {
        "Module": "Laravel\\Modules\\Facades\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.1.1-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
